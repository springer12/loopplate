{
  "restApiRoot": "/api",
  "domain": "loopplate.com",
  "email": "service@loopplate.com",
  "host": "0.0.0.0",
  "legacyExplorer": false,
  "port": 80,
  "cookieSecret": "change~this~text",
  "remoting": {
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "handleErrors": true,
      "debug": true,
      "log": true
    }
  },
  "oauth": {
    "facebook": {
      "clientId": "~",
      "clientSecret": "~"
    },
    "google": {
      "clientId": "~",
      "clientSecret": "~"
    },
    "yahoo": {
      "clientId": "~",
      "clientSecret": "~"
    }
  },
  "maxPageSize": 20,
  "storage": {
    "provider": "amazon",
    "key": "~",
    "keyId": "~",
    "region": "",
    "container": "loopplate-live"
  },
  "getstream": {
    "apiKey": "~",
    "apiSecret": "~",
    "appId": "16574"
  },
  "google": {
    "key": "~"
  },
  "twitter": {
    "key": "~",
    "secret": "~",
    "accessToken": "~",
    "accessTokenSecret": "~"
  },
  "instagram": {
    "clientId": "~",
    "clientSecret": "~"
  },
  "gettyimages": {
    "key": "~",
    "secret": "~"
  },
  "giphy": {
    "key": "~"
  },
  "logs": {
    "transport": "sentry",
    "options": {
      "dsn": "https://~:~@sentry.io/~"
    }
  },
  "meta": {
    "title": "Loopplate - Loopback Node.js framework boilerplate - loopplate.com",
    "description": "Loopplate - Loopback Node.js framework boilerplate - loopplate.com"
  }
}
