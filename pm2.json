{
  "apps": [
    {
      "name": "loopback",
      "script": "./build/server/server.js",
      "node_args": "--harmony",
      "log_file": "./logs/loopback-logs.log",
      "error_file": "./logs/loopback-errs.log",
      "out_file": "./logs/loopback-out.log",
      "pid_file": "./logs/loopback.pid",
      "watch": false
    },
    {
      "name": "send-summary-worker",
      "script": "./build/worker/sendSummary.js",
      "node_args": "--harmony",
      "log_file": "./logs/send-summary-worker-logs.log",
      "error_file": "./logs/send-summary-worker-errs.log",
      "out_file": "./logs/send-summary-worker-out.log",
      "pid_file": "./logs/send-summary-worker.pid",
      "watch": false
    }
  ]
}
