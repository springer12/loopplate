{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "classic",
    "removeComments": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "declaration": true,
    "preserveConstEnums": true,
    "noUnusedLocals": true
    // "noUnusedParameters": true
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}
